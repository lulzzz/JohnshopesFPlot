body { margin-top:0px; margin-bottom:0px; font-family:Arial,sans-serif; background-image:url(img/MandelbrotBright.jpg); }

div.sb p
     { font-family:Arial,sans-serif; font-weight:bold; }
div.sb h4
     { font-family:Arial,sans-serif; font-weight:bold; color:#000099; }
/*
div.sb p,h1,h2,h3,h4,dl,ul,ol,li,td,th,address,blockquote,nobr,b,i
     { font-family:Arial,sans-serif; font-weight:bold; }
*/
div.sb h1 { font-size:14pt; }
div.sb h2 { font-size:12pt; }
div.sb h3 { font-size:9pt; }
div.sb h4 { font-size:9pt; }
div.sb p { font-size:8pt; }

/* div.sb p,dl,ul,ol,li,div,td,th,address,nobr,b,i { font-size:8pt; } */

div.sb a:link { font-weight:bold; text-decoration:underline; }
div.sb a:visited { font-weight:bold; text-decoration:underline; }
div.sb a:active { font-weight:bold; text-decoration:none; }

p,h1,h2,h3,h4,dl,dd,dt,ul,ol,li,div,td,th,address,nobr,i,b
  { font-family:Arial,sans-serif; color:#000000; font-size:10pt; }

h1 { font-size:16pt; color:#000099; }
h2 { font-size:14pt; color:#000099; }
h3 { font-size:11pt; color:#000099; }
h4 { font-size:10pt; color:#000099; }
blockquote { font-family:Times New Roman,Times,Bodonji,serif; }
blockquote p { font-family:Times New Roman,Times,Bodonji,serif;font-size:12pt; }
blockquote h3 { font-family:Times New Roman,Times,Bodonji,serif;font-size:14pt; }
pre { font-family:Courier New,Courier; font-size:10pt; }
tt { font-family:Courier New,Courier; font-size:10pt; }
a:link { font-weight:bold; text-decoration:underline; }
a:visited { font-weight:bold; text-decoration:underline; }
a:active { font-weight:bold; text-decoration:none; }
a.an { text-decoration:none; }
